{
  "name": "barcode-label-maker",
  "version": "1.0.0",
  "description": "Vue 3 barcode label generator",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "node server.js",
    "preview": "vite preview",
    "pm2:dev": "pm2 start ecosystem.config.js --only barcode-label-maker-dev",
    "pm2:prod": "npm run build && pm2 start ecosystem.config.js --only barcode-label-maker-prod",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status"
  },
  "keywords": [
    "vue",
    "barcode",
    "label",
    "generator"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "html2canvas": "^1.4.1",
    "jsbarcode": "^3.11.5",
    "jspdf": "^2.5.1",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.6.2",
    "vite": "^4.4.0"
  }
}